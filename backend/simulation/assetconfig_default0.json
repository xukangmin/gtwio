        "Name" : "Avg Shell Inlet",
        "Equation": "avg([ShellInlet/Temperature,INTERVAL])",
        "Tag": "AVG_SHELL_INLET:INTERVAL"
      },
      {
        "Name" : "Avg Shell Outlet",
        "Equation": "avg([ShellOutlet/Temperature,INTERVAL])",
        "Tag": "AVG_SHELL_OUTLET:INTERVAL"
