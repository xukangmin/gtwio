[
  {
    "AssetName" : "demo_hx_asset(0)",
    "Equations" :
    [
      {
        "Name" : "Avg Shell Inlet",
        "Equation": "Avg([ShellInlet/Temperature])",
        "Tag": "AVG_SHELL_INLET"
      },
      {
        "Name" : "Avg Shell Outlet",
        "Equation": "Avg([ShellOutlet/Temperature])",
        "Tag": "AVG_SHELL_OUTLET"
      },
      {
        "Name" : "Avg Tube Inlet",
        "Equation": "Avg([TubeInlet/Temperature])",
        "Tag": "AVG_TUBE_INLET"
      },
      {
        "Name" : "Avg Tube Outlet",
        "Equation": "Avg([TubeOutlet/Temperature])",
        "Tag": "AVG_TUBE_OUTLET"
      },
      {
        "Name" : "Delta T S",
        "Equation": "[AVG_SHELL_INLET] - [AVG_SHELL_OUTLET]",
        "Tag": "DELTA_T_S"
      },
      {
        "Name" : "Delta T 1",
        "Equation": "[AVG_TUBE_INLET] - [AVG_SHELL_OUTLET]",
        "Tag": "DELTA_T_1"
      },
      {
        "Name" : "Delta T 2",
        "Equation": "[AVG_TUBE_OUTLET] - [AVG_SHELL_INLET]",
        "Tag": "DELTA_T_2"
      },
      {
        "Name" : "LMTD",
        "Equation": "([DELTA_T_1] - [DELTA_T_2]) / log([DELTA_T_1] / [DELTA_T_2])",
        "Tag": "LMTD"
      },
      {
        "Name" : "Delta T Ratio",
        "Equation" : "[DELTA_T_S] / [DELTA_T_S,FIX,BASELINE]",
        "Tag": "DELTA_T_RATIO"
      },
      {
        "Name" : "LMTD Ratio",
        "Equation": "[LMTD] / [LMTD,FIX,BASELINE]",
        "Tag" : "LMTD_RATIO"
      },
      {
        "Name" : "Cleanliness Factor",
        "Equation" : "[DELTA_T_RATIO] * [LMTD_RATIO]",
        "Tag" : "CLEANLINESS_FACTOR",
        "Range" : {
          "UpperLimit": 1.2,
          "LowerLimit": 0
        }
      },
      {
        "Name" : "Cleanliness Factor Uncertainty",
        "Equation" : "[CLEANLINESS_FACTOR] * 0.6",
        "Tag" : "CLEANLINESS_FACTOR_UNCERTAINTY"
      },
      {
        "Name" : "Number Of Data Sets",
        "Equation" : "sum([ShellInlet/Temperature,COUNT,SC,BASELINE,0])",
        "Tag": "SHELLINLET_NUMBER_DATA_SETS"
      },
      {
        "Name" : "ShellInlet Degree of Freedom",
        "Equation" : "[SHELLINLET_NUMBER_DATA_SETS] - 1",
        "Tag": "SHELLINLET_DEGREE_OF_FREEDOM"
      },
      {
        "Name" : "Two Tailed Student T Value",
        "Equation" : "t_value(0.05,[SHELLINLET_DEGREE_OF_FREEDOM])",
        "Tag": "SHELLINLET_STUDENT_T_VALUE"
      },
      {
        "Name" : "Number of sensors",
        "Equation" : "count([ShellInlet/Temperature])",
        "Tag": "SHELLINLET_SENSOR_COUNT"
      },
      {
        "Name" : "ShellInlet STDEV of all data",
        "Equation" : "std([ShellInlet/Temperature,RAW,SC,BASELINE,0])",
        "Tag": "SHELLINLET_STD_ALL_DATA"
      },
      {
        "Name" : "ShellInlet Spatial Bias Uncertainty",
        "Equation" : "[SHELLINLET_STUDENT_T_VALUE] * [SHELLINLET_STD_ALL_DATA] / sqrt([SHELLINLET_SENSOR_COUNT])",
        "Tag": "SHELLINLET_SPATIAL_BIAS_UNCERTAINTY"
      },
      {
        "Name" : "Total Measurement Bias Uncertainty",
        "Equation" : "sqrt(0.1 ^ 2 + 0.05 ^ 2 + [SHELLINLET_STD_ALL_DATA]^2 + [SHELLINLET_SPATIAL_BIAS_UNCERTAINTY]^2)",
        "Tag": "SHELLINLET_TOTAL_MESUREMENT_BIAS_UNCERTAINTY"
      },
      {
        "Name" : "STDEV Data set",
        "Equation" : "std([AVG_SHELL_INLET,RAW,SC,BASELINE,0])",
        "Tag": "SHELLINLET_STDEV_DATA_SET"
      },
      {
        "Name" : "STDEV Sample Mean",
        "Equation" : "[SHELLINLET_STDEV_DATA_SET] / sqrt([SHELLINLET_NUMBER_DATA_SETS])",
        "Tag": "SHELLINLET_STDEV_SAMPLE_MEAN"
      },
      {
        "Name" : "Shell Inlet TMU",
        "Equation" : "sqrt([SHELLINLET_TOTAL_MESUREMENT_BIAS_UNCERTAINTY] ^ 2 + ([SHELLINLET_STUDENT_T_VALUE] * [SHELLINLET_STDEV_SAMPLE_MEAN]) ^2)",
        "Tag": "SHELLINLET_TMU"
      },
      {
        "Name" : "Number Of Data Sets",
        "Equation" : "sum([ShellOutlet/Temperature,COUNT,SC,BASELINE,0])",
        "Tag": "SHELLOUTLET_NUMBER_DATA_SETS"
      },
      {
