        "Tag" : "SHELL_FLOW_RATE"
      },
      {
        "Name" : "Delta T S",
        "Equation": "[AVG_SHELL_INLET] - [AVG_SHELL_OUTLET]",
        "Tag": "DELTA_T_S"
      },
      {
        "Name" : "Delta T 1",
        "Equation": "[AVG_TUBE_INLET] - [AVG_SHELL_OUTLET]",
        "Tag": "DELTA_T_1"
      },
      {
        "Name" : "Delta T 2",
        "Equation": "[AVG_TUBE_OUTLET] - [AVG_SHELL_INLET]",
        "Tag": "DELTA_T_2"
      },
      {
        "Name" : "LMTD",
        "Equation": "([DELTA_T_1] - [DELTA_T_2]) / log([DELTA_T_1] / [DELTA_T_2])",
        "Tag": "LMTD"
      },
      {
        "Name" : "Delta T Ratio",
        "Equation" : "[DELTA_T_S] / [DELTA_T_S,FIRST,BASELINE]",
        "Tag": "DELTA_T_RATIO"
      },
      {
        "Name" : "LMTD Ratio",
        "Equation": "[LMTD] / [LMTD,FIRST,BASELINE]",
        "Tag" : "LMTD_RATIO"
      },
      {
        "Name" : "Mass Flow Ratio",
        "Equation": "[SHELL_FLOW_RATE] / [SHELL_FLOW_RATE,FIRST,BASELINE]",
        "Tag" : "MASS_FLOW_RATIO"        
      },
      {
        "Name" : "Cleanliness Factor",
        "Equation" : "[DELTA_T_RATIO] * [LMTD_RATIO]",
        "Tag" : "CLEANLINESS_FACTOR",
        "Range" : {
          "UpperLimit": 1.2,
          "LowerLimit": 0
        }
      },
      {
        "Name" : "Cleanliness Factor Uncertainty",
        "Equation" : "[CLEANLINESS_FACTOR] * 0.6",
        "Tag" : "CLEANLINESS_FACTOR_UNCERTAINTY"
      },
      {
        "Name" : "Shell Delta T",
        "Equation" : "abs([AVG_SHELL_INLET] - [AVG_SHELL_OUTLET])",
        "Tag" : "SHELL_DELTA_T"
      },
      {
        "Name" : "Shell Specific Heat",
        "Equation" : "-0.00000000000041294*(max([AVG_SHELL_INLET],[AVG_SHELL_OUTLET]))^5+0.000000000314448*(max([AVG_SHELL_INLET],[AVG_SHELL_OUTLET]))^4-0.0000000937872*(max([AVG_SHELL_INLET],[AVG_SHELL_OUTLET]))^3+0.0000142891*(max([AVG_SHELL_INLET],[AVG_SHELL_OUTLET]))^2-0.0010883*(max([AVG_SHELL_INLET],[AVG_SHELL_OUTLET]))+1.03129",
        "Tag" : "SHELL_SPEC_HEAT"
      },
      {
        "Name" : "Shell Density",
        "Equation" : "-0.000000000748921*(max([AVG_SHELL_INLET],[AVG_SHELL_OUTLET]))^4+0.000000527629*(max([AVG_SHELL_INLET],[AVG_SHELL_OUTLET]))^3-0.000188944*(max([AVG_SHELL_INLET],[AVG_SHELL_OUTLET]))^2+0.012246*(max([AVG_SHELL_INLET],[AVG_SHELL_OUTLET]))+62.2069",
        "Tag" : "SHELL_DENSITY"
      },
      {
        "Name" : "Heat Transfer Rate",
        "Equation" : "([SHELL_FLOW_RATE]*[SHELL_DELTA_T]*[SHELL_SPEC_HEAT]*[SHELL_DENSITY])*(0.133681/1)*(60/1)",
        "Tag" : "HEAT_TRANSFER_RATE",
        "Range" : {
          "UpperLimit": 3000000,
          "LowerLimit": 2000000
        }
      },
      {
        "Name" : "Shell Heat Flow Uncertainty",
        "Equation" : "[HEAT_TRANSFER_RATE] * 0.6",
        "Tag" : "HEAT_TRANSFER_RATE_UNCERTAINTY"
      },
      {
        "Name" : "ShellInlet Number Of Data Sets",
        "Equation" : "sum([ShellInlet/Temperature,COUNT,BASELINE])",
        "Tag": "SHELLINLET_NUMBER_DATA_SETS"
      },
      {
